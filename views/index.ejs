<!DOCTYPE html>
<html>
  <head>
    <% include ./header.ejs %>
    <script src="../public/js/jquery-3.2.1.min.js"></script>
    <script>
        $(document).ready(function() {
            var date;
            var colors = ['red', 'white', 'blue', 'black', 'green'];
            var mEvent = function() {
                date = new Date();
                console.log("colors[]", colors[date % 5]);
                $('body').css('background-color', colors[date % 5]);
            }
            setInterval(mEvent, 1000);
        });
    </script>
